{
  "name": "butta_webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "db:generate": "prisma generate --schema=./kitchen/schema.prisma",
    "db:push": "prisma db push --schema=./kitchen/schema.prisma",
    "db:studio": "prisma studio --schema=./kitchen/schema.prisma",
    "db:seed": "node kitchen/scripts/seed-prisma-data.js",
    "db:test": "node kitchen/scripts/test-prisma-connection.js",
    "kitchen:setup": "tsx kitchen/scripts/setup-auth.ts",
    "kitchen:migrate": "node -e \"console.log('Please run the SQL migration manually in Supabase Dashboard')\"",
    "test:kitchen-api": "node kitchen/test-kitchen-api.js",
    "test:kitchen-quick": "tsx kitchen/api/test-runner.ts",
    "kitchen:setup-db": "node kitchen/setup-database.js",
    "test:kitchen-integration": "tsx kitchen/test-integration.ts",
    "verify-setup": "node verify-setup.js",
    "prisma:setup-supabase": "tsx kitchen/scripts/setup-prisma-supabase.ts",
    "prisma:create-tables": "node kitchen/scripts/setup-prisma-tables.js"
  },
  "dependencies": {
    "@prisma/client": "6.12.0",
    "@radix-ui/react-avatar": "^1.1.1",
    "@radix-ui/react-dialog": "^1.1.4",
    "@radix-ui/react-dropdown-menu": "^2.1.4",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@supabase/supabase-js": "^2.48.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/xlsx": "^0.0.36",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "framer-motion": "^12.23.9",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.525.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.1.1",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@shadcn/ui": "^0.0.4",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react-swc": "^3.10.2",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "postcss": "^8.5.6",
    "prisma": "^6.12.0",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4"
